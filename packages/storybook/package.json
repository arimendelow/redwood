{
  "name": "storybook-framework-redwoodjs-vite",
  "version": "7.0.0",
  "description": "Storybook for RedwoodJS with Vite",
  "keywords": [
    "Storybook",
    "RedwoodJS",
    "React",
    "Vite"
  ],
  "bugs": {
    "url": "https://github.com/redwoodjs/redwood/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/storybook"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js"
    },
    "./dist/mocks/MockRouter": "./dist/mocks/MockRouter.jsx",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "build:js": "babel src -d dist --extensions \".js,.jsx,.ts,.tsx\"",
    "build:types": "tsc --build --verbose"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.23.9",
    "@joshwooding/vite-plugin-react-docgen-typescript": "0.3.0",
    "@rollup/pluginutils": "^5.0.2",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/builder-vite": "7.6.10",
    "@storybook/react": "7.6.10",
    "core-js": "3.35.1",
    "magic-string": "^0.30.0",
    "react-docgen": "^7.0.0",
    "unplugin-auto-import": "^0.17.5"
  },
  "devDependencies": {
    "@babel/cli": "7.23.9",
    "@babel/core": "^7.22.20",
    "@types/node": "^18.0.0",
    "typescript": "^5.3.2",
    "vite": "4.5.2"
  },
  "peerDependencies": {
    "@redwoodjs/project-config": "7.0.2",
    "@redwoodjs/router": "7.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.6.10"
  },
  "engines": {
    "node": "=20.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
